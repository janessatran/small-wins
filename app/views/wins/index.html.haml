%section.container
  %h1 Small Wins 
  %h2 A list of small wins I make each day
  %table
    %tr
    - @csv_table.headers.each do |header|
      %th= header
    -@csv_table.each do |row|
      %tr 
      - row.each do |element|
        - if element[1].include?("https")
          %td= link_to element[1], element[1]
        - else
          %td= element[1]
      


    